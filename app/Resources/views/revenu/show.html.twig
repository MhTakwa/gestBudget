{% extends 'utilisateurs/index_revenu.html.twig' %}
{%block add%}
 <form>
<input type="button" value="Nouveau" onclick="window.location.href='{{path('gestion_revenu_new')}}'" />
</form> 
{%endblock%}
{% block body %}
    <h1>Revenu</h1>

    <div class="module-body table"><table cellpadding="0" cellspacing="0" border="0" class="datatable-1 table table-bordered table-striped  display"
       width="100%">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ revenu.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ revenu.libelle }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{% if revenu.date %}{{ revenu.date|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Montant</th>
                <td>{{ revenu.montant }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ revenu.type }}</td>
            </tr>
        </tbody>
    </table></div>

    <ul>
        <li>
            <a href="{{ path('gestion_revenu_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('gestion_revenu_edit', { 'id': revenu.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
